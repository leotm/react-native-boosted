{
  "name": "myapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "setup": "yarn",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "android": "react-native run-android --active-arch-only",
    "android:release": "react-native run-android --active-arch-only --variant release",
    "ios": "react-native run-ios",
    "ios:release": "react-native run-ios --configuration Release",
    "ios:light": "xcrun simctl ui booted appearance light",
    "ios:dark": "xcrun simctl ui booted appearance dark",
    "debug": "flipper-server",
    "doctor": "react-native doctor",
    "lint": "eslint . --ext ts,tsx,yml --cache",
    "lint:fix": "eslint . --ext ts,tsx,yml --cache --fix",
    "lint:config": "eslint --print-config index.ts",
    "prettier:check": "yarn prettier --check src/**/*.{ts,tsx,json} && yarn prettier --check *.{ts,tsx,json} && yarn prettier --write storybook/**/**/*.{ts,tsx}",
    "prettier:write": "yarn prettier --write src/**/*.{ts,tsx,json} && yarn prettier --write *.{ts,tsx,json} && yarn prettier --write storybook/**/**/*.{ts,tsx}",
    "check:all": "yarn tsc && yarn lint && yarn prettier:check && jest",
    "fix": "yarn lint:fix && yarn prettier:write",
    "test": "jest",
    "snap": "jest -u",
    "storybook": "(adb reverse tcp:7007 tcp:7007 || true) && start-storybook -c storybook -p 7007",
    "get-stories": "sb-rn-get-stories --config-path=./.storybook",
    "server": "ts-node node.ts",
    "deno": "deno run --allow-net deno.ts",
    "rename": "yarn react-native-rename",
    "clean": "react-native clean",
    "clean:proj": "react-native clean-project",
    "clean:proj-auto": "react-native clean-project-auto",
    "help": "react-native"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.17.7",
    "@react-native-community/checkbox": "0.5.12",
    "@react-native-community/datetimepicker": "6.3.1",
    "@react-native-community/slider": "4.2.4",
    "@react-navigation/native": "6.0.11",
    "@react-navigation/native-stack": "6.7.0",
    "react": "18.1.0",
    "react-native": "0.70.0-rc.1",
    "react-native-codegen": "0.70.3",
    "react-query": "beta"
  },
  "devDependencies": {
    "@babel/core": "7.18.10",
    "@babel/preset-react": "7.18.6",
    "@babel/runtime": "7.18.9",
    "@curveball/core": "0.19.0",
    "@curveball/router": "0.4.1",
    "@react-native-community/eslint-config": "3.0.3",
    "@storybook/addon-actions": "6.5.9",
    "@storybook/addon-controls": "6.5.9",
    "@storybook/addon-knobs": "6.4.0",
    "@storybook/addon-links": "6.5.9",
    "@storybook/addon-notes": "next",
    "@storybook/addon-ondevice-actions": "6.0.1-beta.7",
    "@storybook/addon-ondevice-backgrounds": "6.0.1-beta.7",
    "@storybook/addon-ondevice-controls": "6.0.1-beta.7",
    "@storybook/addon-ondevice-knobs": "next",
    "@storybook/addon-ondevice-notes": "6.0.1-beta.7",
    "@storybook/addons": "6.5.9",
    "@storybook/eslint-config-storybook": "3.1.2",
    "@storybook/linter-config": "3.1.2",
    "@storybook/react-native": "6.0.1-beta.7",
    "@storybook/react-native-server": "next",
    "@swc/core": "1.2.223",
    "@swc/wasm": "1.2.223",
    "@types/jest": "28.1.6",
    "@types/node": "16.11.47",
    "@types/react": "18.0.15",
    "@types/react-native": "0.69.5",
    "@types/react-test-renderer": "18.0.0",
    "@types/ws": "8.5.3",
    "@typescript-eslint/eslint-plugin": "5.31.0",
    "@typescript-eslint/parser": "5.31.0",
    "babel-jest": "28.1.3",
    "babel-loader": "8.2.5",
    "babel-plugin-transform-inline-environment-variables": "0.4.4",
    "eslint": "8.20.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-airbnb-typescript-prettier": "5.0.0",
    "eslint-config-react-native-typescript": "2.2.6",
    "eslint-plugin-communist-spelling": "1.0.0",
    "eslint-plugin-deprecation": "1.3.2",
    "eslint-plugin-detox": "1.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.7.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-native": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "eslint-plugin-yml": "1.1.0",
    "flipper-server": "0.156.0",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "lint-staged": "13.0.3",
    "metro-react-native-babel-preset": "0.72.0",
    "prettier": "2.7.1",
    "react-codemod": "5.4.3",
    "react-dom": "18.1.0",
    "react-native-clean-project": "4.0.1",
    "react-native-gradle-plugin": "0.70.0",
    "react-native-rename": "2.9.0",
    "react-test-renderer": "18.1.0",
    "reactotron-react-native": "5.0.2",
    "ts-node": "10.9.1",
    "typescript": "beta"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testPathIgnorePatterns": [
      "lib"
    ],
    "setupFiles": [
      "./jest/setup.ts"
    ]
  },
  "codegenConfig": {
    "libraries": []
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "packageManager": "yarn@3.2.2"
}
